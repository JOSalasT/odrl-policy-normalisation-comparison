@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://example.com/iri/policy/c5eeb9a1-58f8-4953-b1b8-8e46a1e76f44> a odrl:Policy ;
    odrl:permission 
        [ a odrl:Permission ;
            odrl:action odrl:watermark ;
            odrl:target <https://w3id.org/dpv/owl#UnverifiedData> ;
            odrl:constraint [ odrl:leftOperand odrl:absoluteSpatialPosition ;
                    odrl:operator odrl:gteq ;
                    odrl:rightOperand 60 ],
                [ odrl:leftOperand odrl:absoluteSpatialPosition ;
                    odrl:operator odrl:lteq ;
                    odrl:rightOperand 90 ] ;
            odrl:target <https://w3id.org/dpv/owl#UnverifiedData> ] ;
    odrl:prohibition 
        [ a odrl:Prohibition ;
            odrl:action odrl:watermark ;
            odrl:target <https://w3id.org/dpv/owl#UnverifiedData> ;
            odrl:constraint [ odrl:leftOperand odrl:absoluteSpatialPosition ;
                    odrl:operator odrl:gt ;
                    odrl:rightOperand 70 ],
                [ odrl:leftOperand odrl:absoluteSpatialPosition ;
                    odrl:operator odrl:lteq ;
                    odrl:rightOperand 80 ] ;
            odrl:target <https://w3id.org/dpv/owl#UnverifiedData> ] .

