@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://example.com/iri/policy/c5eeb9a1-58f8-4953-b1b8-8e46a1e76f44> a odrl:Policy ;
    odrl:permission [ a odrl:Permission ;
            odrl:action odrl:watermark ;
            odrl:target <https://w3id.org/dpv/owl#UnverifiedData> ;
            odrl:constraint [ odrl:leftOperand odrl:absoluteSpatialPosition ;
                    odrl:operator odrl:gteq ;
                    odrl:rightOperand 2 ],
                [ odrl:leftOperand odrl:media ;
                    odrl:operator odrl:gteq ;
                    odrl:rightOperand 49 ],
                [ odrl:leftOperand odrl:absoluteSpatialPosition ;
                    odrl:operator odrl:eq ;
                    odrl:rightOperand 53 ],
                [ odrl:leftOperand odrl:virtualLocation ;
                    odrl:operator odrl:neq ;
                    odrl:rightOperand 22 ] ],
        [ a odrl:Permission ;
            odrl:action odrl:uninstall ;
            odrl:assignee <https://w3id.org/dpv/owl#FireDepartment> ;
            odrl:constraint [ odrl:leftOperand odrl:virtualLocation ;
                    odrl:operator odrl:eq ;
                    odrl:rightOperand 70 ],
                [ odrl:leftOperand odrl:virtualLocation ;
                    odrl:operator odrl:lt ;
                    odrl:rightOperand 75 ],
                [ odrl:leftOperand odrl:absoluteSpatialPosition ;
                    odrl:operator odrl:eq ;
                    odrl:rightOperand 51 ],
                [ odrl:leftOperand odrl:media ;
                    odrl:operator odrl:lt ;
                    odrl:rightOperand 79 ] ];
    odrl:prohibition [ a odrl:Prohibition ;
            odrl:action odrl:watermark ;
            odrl:target <https://w3id.org/dpv/owl#UnverifiedData> ;
            odrl:constraint [ odrl:leftOperand odrl:absoluteSpatialPosition ;
                    odrl:operator odrl:gteq ;
                    odrl:rightOperand 53 ],
                [ odrl:leftOperand odrl:media ;
                    odrl:operator odrl:neq ;
                    odrl:rightOperand 90 ],
                [ odrl:leftOperand odrl:virtualLocation ;
                    odrl:operator odrl:neq ;
                    odrl:rightOperand 68 ],
                [ odrl:leftOperand odrl:relativePosition ;
                    odrl:operator odrl:gt ;
                    odrl:rightOperand 26 ] ;
             ] .

